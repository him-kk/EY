{
  "name": "LeadSubmission",
  "type": "object",
  "properties": {
    "full_name": {
      "type": "string",
      "description": "Customer's full name"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Customer's email address"
    },
    "phone": {
      "type": "string",
      "description": "Customer's phone number"
    },
    "vehicle_make": {
      "type": "string",
      "description": "Vehicle manufacturer"
    },
    "vehicle_model": {
      "type": "string",
      "description": "Vehicle model"
    },
    "vehicle_year": {
      "type": "string",
      "description": "Vehicle year"
    },
    "message": {
      "type": "string",
      "description": "Additional message from customer"
    },
    "status": {
      "type": "string",
      "enum": [
        "new",
        "contacted",
        "converted"
      ],
      "default": "new",
      "description": "Lead status"
    }
  },
  "required": [
    "full_name",
    "email",
    "phone",
    "vehicle_make",
    "vehicle_model",
    "vehicle_year"
  ]
}